{
    "manifest_version": 3,
    "name": "Open Link",
    "version": "1.0",
    "description": "",
    "content_scripts": [
        {
            "js": [
                "main.js"
            ],
            "matches": [
                "https://remote.bmlabserver.com/*",
                "http://remote.bmlabserver.com/*",
                "http://192.168.1.245:96/*"
            ],
            "run_at": "document_end"
        }
    ],
    "icons": {
        "16": "open-link.png",
        "32": "open-link.png",
        "48": "open-link.png",
        "128": "open-link.png"
    }
}